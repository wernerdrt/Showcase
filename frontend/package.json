    "@angular/language-service": "4.3.1",
    "@compodoc/compodoc": "1.0.0-beta.13",
    "@pact-foundation/karma-pact": "^2.1.3",
    "@pact": "^4.3.2",
    "@pact-foundation/pact-node": "^6.5.0",
    "@pact-foundation/pact-web": "5.3.0",
