{
  "name": "Educama-Frontend",
  "version": "1.0.0",
  "description": "Angular2 Web-UI for Educama Project",
  "license": "Apache-2.0",
  "scripts": {
    "all": "npm run clean && npm install && npm run lint && npm run prod && npm run test && npm run docs && npm run zip",
    "clean": "rm ./node_modules -rf",
    "serve": "ng serve",
    "build": "ng build",
    "prod": "ng build --prod",
    "test": "ng test -sr --cc=true",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "docs": "./node_modules/.bin/compodoc -p tsconfig.json -d docs",
    "zip": "(cd dist/ && zip -r educama-frontend.zip *) && (zip ./dist/educama-frontend.zip Staticfile)"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "4.3.1",
    "@angular/common": "4.3.1",
    "@angular/compiler": "4.3.1",
    "@angular/core": "4.3.1",
    "@angular/forms": "4.3.1",
    "@angular/http": "4.3.1",
    "@angular/platform-browser": "4.3.1",
    "@angular/platform-browser-dynamic": "4.3.1",
    "@angular/router": "4.3.1",
    "@ngrx/effects": "4.0.1",
    "@ngrx/store": "4.0.0",
    "@ngrx/store-devtools": "4.0.0",
    "core-js": "2.4.1",
    "font-awesome": "4.7.0",
    "lodash": "4.17.4",
    "moment": "2.15.1",
    "ng2-translate": "5.0.0",
    "npm": "^6.0.0",
    "primeng": "4.0.0",
    "reflect-metadata": "0.1.8",
